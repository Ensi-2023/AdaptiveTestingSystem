<UserControl x:Class="AdaptiveTestingSystem.Control.CustomControl.StatusConnectControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AdaptiveTestingSystem.Control.CustomControl"
             mc:Ignorable="d" Name="root"
             MaxHeight="40" MaxWidth="40">
    <Grid>
        <Ellipse Height="20" Width="20" Fill="{Binding ElementName=root,Path=BorderColor}">

        </Ellipse>

        <Ellipse Height="19" Width="19" Fill="{Binding ElementName=root,Path=BarColor}">

        </Ellipse>
    </Grid>
</UserControl>
